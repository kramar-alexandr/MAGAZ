global
procedure CalcNewPrice(val oldprice,string extra,var val newprice)
BEGIN
  string 255 tstr;
  val t,valv;
            
  newprice = blankval;
  if (blank(extra)) then begin
    newprice = oldprice;
    goto LCalcNewPrice;
  end;
  if (blank(oldprice)) then begin
    goto LCalcNewPrice;
  end;
  tstr = extra;
  if (Mid(tstr,len(tstr)-1,1)=="%") then begin  
    tstr = Left(tstr,len(tstr)-1);
    valv = StringToVal(tstr,M4Val);
    t = oldprice*valv;
    t = t/100;
    newprice = oldprice + t;
  end else begin
    valv = StringToVal(tstr,M45Val);
    newprice = oldprice + valv;
  end;
LCalcNewPrice:;
  RETURN;
END;

