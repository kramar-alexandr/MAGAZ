global
updating procedure PrintStockMovInvoices()
begin
  record StockMovVc SMr;
  record RcVc RepSpec;
  boolean res;
  Integer wn;
  longint errorlongint;
  string 255 errormsg;
  
  res = true;
  wn = CurWindow;
  if (WindowState(wn)==Rs_normal) then begin
    DeselectWindow(wn,true);
    GetWindowRecord(wn,SMr);
    if (HasLocalization("PRT")) then begin
      switch (SMr.StockMovType) begin
        case kStockMovTypeConsigment: res = true;
        otherwise
          MessageBox(39194,": " & StringFromSet(564,SMr.StockMovType) & chr(13) & chr(10) & USetStr(8329) & ": " & StringFromSet(564,kStockMovTypeConsigment));
          res = false;
          goto LPrintStockMovInvoices;
      end;
    end;
    if (SMr.OrderNr==-1) then begin 
      res = false;
      MessageBox(39197,"");
      goto LPrintStockMovInvoices;
    end;
    if (SMr.InvoiceNr!=-1 ) then begin
      res = PrintDocument(SMr,"StockMovInvForm",false);
      res = false;
      goto LPrintStockMovInvoices;
    end;
    if (nonblank(SMr.ThrouLocation)) then begin 
      if (SMr.SentOKFlag==0) then begin
        res = false;
        MessageBox(39191,"");
        goto LPrintStockMovInvoices;
      end;
    end;
    if (SMr.OKFlag==0) then begin
      res = false;
      MessageBox(39191,"");
      goto LPrintStockMovInvoices;
    end;
    if (res) then begin
      RemotePrintStockMovInvoices(SMr,errorlongint,errormsg,RepSpec);
      if (errorlongint==0) then begin 
        SetWindowState(wn,Rs_update);
        PutWindowRecord(wn,SMr);
        SetWindowState(wn,Rs_normal);
        if (res) then begin   
          res = PrintDocument(SMr,"StockMovInvForm",false);
        end;       
      end else begin
        MessageBox(errorlongint,errormsg);
      end; 
    end;
  end else begin
    MessageBox(39193,"");
  end;
LPrintStockMovInvoices:;  
  return;
end;





function Boolean FindInMathGeneral( string RegisterName, integer mwn, string search, integer from, var integer RowNumber)
begin					//added -----------------------Dima 12.01.2015
  row SHVc SHrw;
	row StockMovVc SMovrw;
	record SHVc SHr;
	record StockMovVc SMovr;
  boolean res;
  Integer i,rwcnt;

  res=false;

  switch (RegisterName) begin
    case "SHVc":
								GetWindowRecord(mwn,SHr); 
								rwcnt = MatRowCnt(SHr);
								for (i = from;i<rwcnt;i=i+1) begin
										MatRowGet(SHr,i,SHrw);
											if(search==SHrw.ArtCode) then begin
												RowNumber = i;
												res = true;
												goto LFindInMathGeneral;
											end;		  
								 end;								
       
    case "StockMovVc": 
								GetWindowRecord(mwn,SMovr);
								rwcnt = MatRowCnt(SMovr);
								for (i = from;i<rwcnt;i=i+1) begin
										MatRowGet(SMovr,i,SMovrw);
											if(search==SMovrw.ArtCode) then begin
												RowNumber = i;
												res = true;
												goto LFindInMathGeneral;
											end;		  
								 end;								
      
  end;  

  LFindInMathGeneral:;	
  FindInMathGeneral = res;
return;
end;





global 	//Edit-------------Dima  12.01.2015
updating procedure StockMove_AddOneItemTo()
begin
	record INVc INr,INr1;
	record StockMovVc SMr;
	row StockMovVc SMrw;
	record BarcodeVc Barcoder;
	record RcVc RepSpec;
	integer wn,mwn,RowNumber,curRow,rwcnt;
	Boolean key,put;
	string 30 searchstr;
	integer sQuant,sSentQuant;

	sSentQuant = 3;	
  sQuant = 2;

  wn=CurWindow;
  GetWindowRecord(wn,RepSpec);
  mwn = RepSpec.flags[0];
  DeselectWindow(mwn,false);
  GetWindowRecord(mwn,SMr);
	rwcnt = MatRowCnt(SMr);


	searchstr = RepSpec.f2;


	INr1.Code = searchstr;
	if(ReadFirstMain(INr1,1,true)) then begin
		INr.Code = INr1.Code;
		key = true;
	end else begin
		RecordClear(INr1);
		INr1.BarCode = searchstr;
		if(ReadFirstKey("BarCode",INr1,1,true)) then begin
			INr.Code = INr1.Code;
			key = true;
		end else begin
			RecordClear(INr1);
			INr1.AlternativeCode = searchstr;
			if(ReadFirstKey("AlternativeCode",INr1,1,true)) then begin
				INr.Code = INr1.Code;
				key = true;
			end else begin
				Barcoder.Barcode = searchstr;
				if (ReadFirstMain(Barcoder,1,true)) then begin
					INr.Code = Barcoder.Itemcode;
					key = true;					
				end;
			end;
		end;
	end;

	curRow = 0;
	RowNumber = 0;
	put = false;

	if(key) then begin
			while	(curRow < rwcnt) begin	 //we try to find the same item in next rows
				if(FindInMathGeneral("StockMovVc",mwn,INr.Code,curRow,RowNumber)==true) then begin
						MatRowGet(SMr,RowNumber,SMrw);
								if (SMrw.SentQuant < SMrw.OrdQuant) then begin
									SMrw.SentQuant = SMrw.SentQuant+1;
									put = true;
								end;
								if (put) then begin
										MatRowPut(SMr,RowNumber,SMrw);
										StockMovVc_PasteSentQuant(SMr,RowNumber);
										PutWindowRecord(mwn,SMr);
			  						WindowFieldGoto(mwn,SMr,RowNumber,"SentQuant",true);
										DeselectWindow(mwn,false);									
										WindowFieldGoto(wn,RepSpec,-1,"f1",false);
										goto LStockMove_AddOneItemTo;
								end else begin
          					curRow = curRow+1; //we try to find the same item in next rows
								end;

					end else begin
						Beep;
						MessageBox(50019,RepSpec.f2);
						goto LStockMove_AddOneItemTo;
					end;
			end;
			Beep;
			MessageBox(50019,RepSpec.f2);
	end else begin
			Beep;
			MessageBox(50020,"");
	end;	

	LStockMove_AddOneItemTo:;

return;  
end;




procedure UnSelect(integer wn)//Edit----------------------Dima  16.02.2015
Begin
	record StockMovVc SMr;
	row StockMovVc SMrw;
	record SHVc SHr;
	row SHVc SHrw;	
	record RcVc RepSpec;
	integer mwn,i,rwcnt;
	string 20 windowClass;	

  GetWindowRecord(wn,RepSpec);
  mwn = RepSpec.flags[0];

	windowClass = GetWindowClass(mwn);

  switch(windowClass) begin
  	case "StockMovDClass":		
  		GetWindowRecord(mwn,SMr);
			rwcnt = MatRowCnt(SMr);	
				for(i=0;i<rwcnt;i=i+1) begin
						MatRowGet(SMr,i,SMrw);
						MatRowPut(SMr,i,SMrw);
						PutWindowRecord(mwn,SMr);
						goto LUnSelect;
				end;
		case "SHDClass":		
  		GetWindowRecord(mwn,SHr);
			rwcnt = MatRowCnt(SHr);	
				for(i=0;i<rwcnt;i=i+1) begin
						MatRowGet(SHr,i,SHrw);
						MatRowPut(SHr,i,SHrw);
						PutWindowRecord(mwn,SHr);
						goto LUnSelect;
				end;
	end;			
					
LUnSelect:;
return;
end;




