function string 40 OnHotelGuestTouchScreenOClassSpecPasteName(Integer wn,string defpsname)
begin
  string 255 psname;
  record RcVc RepSpec;
  
  GetWindowRecord(wn,RepSpec);
  psname = defpsname;
  switch (WindowActiveField(wn)) begin
    case "f2":
      if (blank(RepSpec.f1)) then begin
        psname = "CUGuestSClass";
      end else begin
        psname = "GuestSClass";
      end;
    case "f3":
