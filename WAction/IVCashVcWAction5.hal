global //Edit***************************Sasha2,11:04 14.09.2016 {
function Boolean IfAllowedChangeArtCodePrice(string artCode)
begin
  Boolean res;
  record INVc INr;
  record ITVc ITr;
    
    INr.Code = artCode;
    if (ReadFirstMain(INr,1,true)) then begin
      switch (INr.ChangePrice) begin
        case 0:
          if (NonBlank(INr.Group)) then begin
            ITr.Code = INr.Group;
            if (ReadFirstMain(ITr,1,true)) then begin
              switch (ITr.ChangePrice) begin
                case 0:
                  res = false;
                case 1:
                  res = true;
              end;
            end;
          end;
        case 1:
          res = false;
        case 2:
          res = true;
      end;
    end;
  
    IfAllowedChangeArtCodePrice = res;
  return;
end; //Edit***************************Sasha2,11:04 14.09.2016 }

