updating procedure PrintIVCashFiscalReceiptAndUpdatePrintFlag() //Edit***************************Sasha2,16:19 13.06.2016 {
begin
	record IVCashVc IVCashr;
	integer wn;
	boolean res,increasePrintCount;

  wn = CurWindow;
  if (GetWindowFileName(wn)=="IVCashVc") then begin
    GetWindowRecord(wn,IVCashr);
    if (IVCashr.MakeFiscalReceiptF==1) then begin
      res = MakeFiscalReceipt(IVCashr);  
      increasePrintCount = res;
    end else begin
      res = true;
      increasePrintCount = false;
    end;
    
    if (res==false) then begin
  		MessageBox(50004,"");
  	end;
    if (res and increasePrintCount) then begin
      if (IVCashr.FiscalFlag < 1) then begin
        IVCashr.FiscalFlag = 1;
      end;
      IVCashr.PrintFlagCounter = IVCashr.PrintFlagCounter + 1;
      RECORDSTORE(IVCashr,true);
      PutWindowRecord(wn,IVCashr);
      windowdook(wn,0);
      
    end;
  end;
  
end; //Edit***************************Sasha2,16:19 13.06.2016 }


