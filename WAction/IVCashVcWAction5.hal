      if (mwn) then begin
        switch (GetWindowClass(mwn)) begin
          case "NPTSIVCashDClass":
            GetWindowRecord(mwn,IVCashr);
            if (PriceLookupVClassIVCashVc_PasteArtCode(RepSpec,IVCashr)==false) then begin
