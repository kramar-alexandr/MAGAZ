function Integer HashTouchScreenPageCode(string tstr)
begin
  LongInt sum;
  Integer i,res;
  
 sum = 0;
  for (i=0;i<len(tstr);i=i+1) begin
    sum = sum + asc(Mid(tstr,i,1));
  end;
  res = BitAnd(sum,65536-1);
  HashTouchScreenPageCode = res;
  return;
end;

