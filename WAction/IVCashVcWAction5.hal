      otherwise
        if (IVCrw.stp==kInvoiceRowTypeReturnIVNr) then begin 
          orgnr = IVCrw.OrgIVNr;
        end;      
    end;
  end;
  
  ClearRow(IVCr,IVCrw,rt);
