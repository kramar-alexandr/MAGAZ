  wn = CurWindow;
  if (wn!=0) then begin
    switch (GetWindowFileName(wn)) begin
      case "RcVc":
        DeselectWindow(wn,false);
        GetWindowRecord(wn,RepSpec);
        if (RepSpec.flags[0]==1) then begin
          RepSpec.flags[0] = 0;
