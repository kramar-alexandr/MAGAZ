  wn = CurWindow;
  line = SelectedListLine(wn);
  if (line>=0) then begin
    if (UserCanAction("AllowNPTSIVRowsVoid",IsStandardProduct==true)==false) then begin
      POSManagersAction(wn,"VoidRow");
