            if (ReadFirstMain(orgIVCr,1,true)) then begin
              rwcnt = MatRowCnt(orgIVCr);
              for (i=0;i<rwcnt;i=i+1) begin
                MatRowGet(orgIVCr,i,orgIVCrw);
                if (orgIVCrw.stp==kInvoiceRowTypeCreditCardPayment) then begin 
                  transactionid = orgIVCrw.CCTransID;
                  i = rwcnt;
                end;
              end;
            end;

            if (blank(transactionid)) then begin
              goto LPOSButtonTypeFullPayment;
            end;
//            VerifoneBanksys_Void(IVCrw.Sum,pmcode,label,transactionid,"",vwn);
          end else begin
            WorldPayCreditCard_Purchase(IVCr.SerNr,IVCr.InvType,IVCrw.Sum,pmcode,label,vwn);
          end;
          goto LPOSButtonTypeFullPayment;
        case kLocalCCTerminalWorldPay:
          vRepSpec.UsedOnly = kLocalCCTerminalWorldPay;
          vRepSpec.ArtMode = 1;
          vRepSpec.long1 = IVCr.SerNr;
          vRepSpec.AccStr = pmcode;
          vRepSpec.f5 = label;
          vRepSpec.CurncyCode = "";
          vwn = OpenWindow("CCPayVerifoneProcessingWClass",0,wn,"","",vRepSpec);
          DeselectWindow(vwn,false);
          if (IVCrw.Sum<0) then begin
            orgIVCr.SerNr = orgnr;
            if (ReadFirstMain(orgIVCr,1,true)) then begin
              rwcnt = MatRowCnt(orgIVCr);
              for (i=0;i<rwcnt;i=i+1) begin
                MatRowGet(orgIVCr,i,orgIVCrw);
                if (orgIVCrw.stp==kInvoiceRowTypeCreditCardPayment) then begin 
                  transactionid = orgIVCrw.CCTransID;
                  i = rwcnt;
                end;
              end;
            end;

            if (blank(transactionid)) then begin
              goto LPOSButtonTypeFullPayment;
            end;
//            VerifoneBanksys_Void(IVCrw.Sum,pmcode,label,transactionid,"",vwn);
